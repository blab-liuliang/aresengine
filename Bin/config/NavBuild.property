<!--note
		1.headers -title
		2.item    -standItem attribute
              property - propery name
              widget   - widget type use for edit
-->
<cfg>
  <headers>
    <header text="Property" />
    <header text="Value" />
  </headers>

  <item text="Rasterization" row="0" col="0">
    <item text="Cell Size" row="0" col="0" />
