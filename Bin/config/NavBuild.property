  </item>
  <item text="Detail Mesh" row="4" col="0">
    <item text="Sample Distance" row="0" col="0" />
    <item text="" row="0" col="1" property="Sample Distance" widget="spinBox" />
    <item text="Max Sample Error" row="1" col="0" />
