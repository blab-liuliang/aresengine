    <item text="Radius" row="1" col="0" />
    <item text="" row="1" col="1" property="Radius" widget="doubleSpinBox" />
    <item text="Max Climb" row="2" col="0" />
    <item text="" row="2" col="1" property="Max Climb" widget="doubleSpinBox" />
    <item text="Max Slope" row="3" col="0" />
